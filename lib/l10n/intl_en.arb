{
  "signUpTitle": "Sign up for {nameOfTheApp} {when}",
  "@signUpTitle": {
    "description": "The title people see when they open the app for the first time.",
    "placeholders": {
      "nameOfTheApp": {
        "type": "String",
        "example": "TikTok"
      },
      "when": {
        "type": "DateTime",
        "format": "yMEd / MMMMEEEEd",
        "isCustomDateFormat": "true"
      }
    }
  },
  "loginTitle": "Log in to {nameOfTheApp}",
  "@loginTitle": {
    "description": "The title people see when they open the app for the log in screen.",
    "placeholders": {
      "nameOfTheApp": {
        "type": "String",
        "example": "TikTok"
      }
    }
  },
  "signUpSubtitle": "Create a profile, follow other accounts, make your own {videoCount, plural, =0{no videos} =1{video} other{videos}}, and more.",
  "emailPhoneButton": "Use phone or email",
  "facebookButton": "Continue with Facebook",
  "appleButton": "Continue with Apple",
  "googleButton": "Continue with Google",
  "alreadyHaveAnAccount": "Already have an account? ",
  "login": "Log in {gender, select, male{sir} female{madam} other{human}}",
  "likeCount": "{value}",
  "@likeCount": {
    "description": "Anything you want",
    "placeholders": {
      "value": {
        "type": "int",
        "format": "compact"
      }
    }
  },
  "commentCount": "{value}",
  "@commentCount": {
    "description": "Anything you want",
    "placeholders": {
      "value": {
        "type": "int",
        "format": "compact"
      }
    }
  },
  "commentTitle": "{value} {value2, plural, =1{comment} other{comments}}",
  "@commentTitle": {
    "description": "Anything you want",
    "placeholders": {
      "value": {
        "type": "int",
        "format": "compact"
      },
      "value2": {
        "type": "int"
      }
    }
  }
}
